<Window x:Class="DTRReportWebWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="DTRReportWebWindow" Height="598" Width="695" Loaded="Window_Loaded" WindowState="Maximized">
    <Grid>
        <Grid.Background>
            <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.ControlColorKey}}"/>
        </Grid.Background>
        <WebBrowser x:Name="wb_report" Margin="10,44,10,10"/>
        <Button x:Name="btnPrint" Content="Print" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="15,10,0,0"/>
        <Button x:Name="btnClose" Content="Close" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="95,10,0,0"/>

    </Grid>
</Window>
